= useState([]);\n  const [loading, setLoading] = useState(true);\n  const [loading2, setLoading2] = useState(true);\n\n  const dispatch = useDispatch();\n\n  const addProduct = (product) => {\n    dispatch(addProduct(product));\n  };\n\n  useEffect(() => {\n    const getProduct = () => {\n      setLoading(true);\n      setLoading2(true);\n      const product = products.find((p) => p.id === parseInt(id));\n      setProduct(product);\n      setLoading(false);\n\n      const similarProducts = products.filter(\n        (p) => p.category === product.category && p.id !== product.id\n      );\n      setSimilarProducts(similarProducts);\n      setLoading2(false);\n    };\n    getProduct();\n  }, [id]);\n\n  const Loading = () => {\n    return (\n      <div className=\"container my-5 py-2\">\n        <div className=\"row\">\n          <div className=\"col-md-6 py-3\">\n            <Skeleton height={400} width={400} />\n          </div>\n          <div className=\"col-md-6 py-5\">\n            <Skeleton height={30} width={250} />\n            <Skeleton height={90} />\n            <Skeleton height={40} width={70} />\n            <Skeleton height={50} width={110} />\n            <Skeleton height={120} />\n            <Skeleton height={40} width={110} inline={true} />\n            <Skeleton className=\"mx-3\" height={40} width={110} />\n          </div>\n        </div>\n      </div>\n    );\n  };\n\n  const ShowProduct = () => {\n    return (\n      <div className=\"container my-5 py-2\">\n        <div className=\"row\">\n          <div className=\"col-md-6 col-sm-12 py-3\">\n            <img\n              className=\"img-fluid\"\n              src={product.image}\n              alt={product.title}\n              width=\"400px\"\n              height=\"400px\"\n            />\n          </div>\n          <div className=\"col-md-6 col-md-6 py-5\">\n            <h4 className=\"text-uppercase text-muted\">{product.category}</h4>\n            <h1 className=\"display-5\">{product.title}</h1>\n            <p className=\"lead\">\n              {product.rating && product.rating.rate}{\" \"}\n              <i className=\"fa fa-star\"></i>\n            </p>\n            <h3 className=\"display-6 my-4\">${product.price}</h3>\n            <p className=\"lead\">{product.description}</p>\n            <button\n              className=\"btn btn-outline-dark\"\n              onClick={() => addProduct(product)}\n            >\n              Add to Cart\n            </button>\n            <Link to=\"/cart\" className=\"btn btn-dark mx-3\">\n              Go to Cart\n            </Link>\n          </div>\n        </div>\n      </div>\n    );\n  };\n\n  const Loading2 = () => {\n    return (\n      <div className=\"my-4 py-4\">\n        <div className=\"d-flex\">\n          <div className=\"mx-4\">\n            <Skeleton height={400} width={250} />\n          </div>\n          <div className=\"mx-4\">\n            <Skeleton height={400} width={250} />\n          </div>\n          <div className=\"mx-4\">\n            <Skeleton height={400} width={250} />\n          </div>\n          <div className=\"mx-4\">\n            <Skeleton height={400} width={250} />\n          </div>\n        </div>\n      </div>\n    );\n  };\n\n  const ShowSimilarProduct = () => {\n    return (\n      <div className=\"py-4 my-4\">\n        <div className=\"d-flex\">\n          {similarProducts.map((item) => {\n            return (\n              <div key={item.id} className=\"card mx-4 text-center\">\n                <img\n                  className=\"card-img-top p-3\"\n                  src={item.image}\n                  alt=\"Card\"\n                  height={300}\n                  width={300}\n                />\n                <div className=\"card-body\">\n                  <h5 className=\"card-title\">\n                    {item.title.substring(0, 15)}...\n                  </h5>\n                </div>\n                <div className=\"card-body\">\n                  <Link\n                    to={\"/product/\" + item.id}\n                    className=\"btn btn-dark m-1\"\n                  >\n                    Buy Now\n                  </Link>\n                  <button\n                    className=\"btn btn-dark m-1\"\n                    onClick={() => addProduct(item)}\n                  >\n                    Add to Cart\n                  </button>\n                </div>\n              </div>\n            );\n          })}\n        </div>\n      </div>\n    );\n  };\n\n  return (\n    <>\n      <Navbar />\n      <div className=\"container\">\n        <div className=\"row\">{loading ? <Loading /> : <ShowProduct />}</div>\n        <div className=\"row my-5 py-5\">\n          <div className=\"d-none d-md-block\">\n            <h2 className=\"\">You may also Like</h2>\n            <Marquee pauseOnHover={true} pauseOnClick={true} speed={50}>\n              {loading2 ? <Loading2 /> : <ShowSimilarProduct />}\n            </Marquee>\n          </div>\n        </div>\n      </div>\n      <Footer />\n    </>\n  );\n};\n\nexport default Product;\n","/Users/darynafesyk/Desktop/React_E-Commerce-jewelry/src/pages/Cart.jsx",[],[],"/Users/darynafesyk/Desktop/React_E-Commerce-jewelry/src/redux/reducer/index.js",[],[],"/Users/darynafesyk/Desktop/React_E-Commerce-jewelry/src/pages/Checkout.jsx",[],[],"/Users/darynafesyk/Desktop/React_E-Commerce-jewelry/src/pages/ContactPage.jsx",[],[],"/Users/darynafesyk/Desktop/React_E-Commerce-jewelry/src/pages/PageNotFound.jsx",[],[],"/Users/darynafesyk/Desktop/React_E-Commerce-jewelry/src/pages/Products.jsx",[],[],"/Users/darynafesyk/Desktop/React_E-Commerce-jewelry/src/pages/AboutPage.jsx",["168"],[],"/Users/darynafesyk/Desktop/React_E-Commerce-jewelry/src/components/index.js",[],[],"/Users/darynafesyk/Desktop/React_E-Commerce-jewelry/src/components/Products.jsx",["169"],[],"import React, { useState, useEffect } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport { addCart } from \